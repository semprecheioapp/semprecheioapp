# === CONFIGURAÇÕES SUPABASE ===
# Obtenha essas credenciais em: https://supabase.com/dashboard
DATABASE_URL=postgresql://postgres:SUA_SENHA@db.SEU_PROJETO.supabase.co:5432/postgres
SUPABASE_URL=https://SEU_PROJETO.supabase.co
SUPABASE_KEY=SUA_CHAVE_ANON_AQUI

# === CONFIGURAÇÕES DE PRODUÇÃO ===
NODE_ENV=development
PORT=5000

# === CONFIGURAÇÕES DE SEGURANÇA ===
SESSION_SECRET=gere_uma_chave_secreta_aleatoria_aqui
RATE_LIMIT_MAX=50
RATE_LIMIT_WINDOW=900000

# === PROTEÇÃO DE DADOS (LGPD) ===
ENABLE_DATA_SANITIZATION=true
SANITIZATION_LOG_LEVEL=info
STRICT_SANITIZATION_MODE=true

# === AUTENTICAÇÃO JWT ===
JWT_SECRET=gere_uma_chave_jwt_super_secreta_aqui_com_pelo_menos_32_caracteres
JWT_EXPIRES_IN=12h
REFRESH_TOKEN_EXPIRES_IN=7d

# === CONFIGURAÇÕES OPCIONAIS ===
LOG_LEVEL=info
BACKUP_ENABLED=true

# === INSTRUÇÕES ===
# 1. Copie este arquivo para .env
# 2. Substitua os valores pelos seus dados reais do Supabase
# 3. NUNCA commite o arquivo .env no git
# 4. Mantenha suas credenciais seguras
